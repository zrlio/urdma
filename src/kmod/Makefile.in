SOURCES = ae.c backports.h cm.c cm.h debug.c debug.h main.c mem.c \
	  obj.c obj.h qp.c urdma.h verbs.c verbs.h Kbuild

ifeq ($(WANT_DEBUG),yes)
export CONFIG_URDMA_DEBUG = y
endif

kmod_prefix=@kmod_prefix@

.PHONY: all
all:
	for i in $(SOURCES); do if [ ! -e $${i} ]; then ln -s @srcdir@/$${i} $${i}; fi; done
	if [ ! -e urdma_kabi.h ]; then ln -s @top_srcdir@/include/urdma_kabi.h .; fi
	if [ ! -e proto_trp.h ]; then ln -s @top_srcdir@/include/proto_trp.h .; fi
	make -C @KERNELDIR@ M=`pwd` top_srcdir=$(realpath @top_srcdir@) PACKAGE_VERSION='\"@PACKAGE_VERSION@\"' modules

Makefile: @srcdir@/Makefile.in
	cd @top_builddir@; ./config.status

.PHONY: distdir
distdir:
	for i in $(SOURCES); do cp @srcdir@/$${i} $(distdir)/$${i}; done

.PHONY: install
install: install-exec install-data

.PHONY: install-exec
install-exec:
	make -C @KERNELDIR@ M=`pwd` INSTALL_MOD_PATH=$(DESTDIR)/$(kmod_prefix) modules_install

install-data:

uninstall:

install-dvi:

install-html:

install-info:

install-ps:

install-pdf:

installdirs:

check:

installcheck:

.PHONY: mostlyclean
mostlyclean:
	make -C @KERNELDIR@ M=`pwd` clean

.PHONY: clean
clean: mostlyclean
	for i in $(SOURCES); do if [ -h $${i} ]; then $(RM) $${i}; fi; done
	$(RM) urdma_kabi.h

.PHONY: distclean
distclean: clean
	$(RM) Makefile

.PHONY: maintainer-clean
maintainer-clean: distclean

dvi:

pdf:

ps:

info:

html:

tags:

.PHONY: ctags
ctags: Makefile
	ctags $(SOURCES)
